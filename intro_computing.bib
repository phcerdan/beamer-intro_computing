	note = {00006},
	keywords = {SAXS},
	pages = {2583--2590},
	file = {ACS Full Text Snapshot:/home/phc/.mozilla/firefox/18x8zg4b.default/zotero/storage/J3H5ZF7R/bm200578d.html:text/html;Schuster_Using_SAXS_to_Reveal_the_Degree_of_Bundling_in_the_Polysaccharide_Junction_2011_Biomacromolecules.pdf:/home/phc/Dropbox/ResearchPapers/Biomacromolecules/2011/Schuster_Using_SAXS_to_Reveal_the_Degree_of_Bundling_in_the_Polysaccharide_Junction_2011_Biomacromolecules.pdf:application/pdf}
