@article{didonna_nonaffine_2005,
	title = {Nonaffine {Correlations} in {Random} {Elastic} {Media}},
	volume = {72},
	issn = {1539-3755, 1550-2376},
	url = {http://arxiv.org/abs/cond-mat/0506456},
	doi = {10.1103/PhysRevE.72.066619},
	abstract = {Materials characterized by spatially homogeneous elastic moduli undergo affine distortions when subjected to external stress at their boundaries, i.e., their displacements \${\textbackslash}uv ({\textbackslash}xv)\$ from a uniform reference state grow linearly with position \${\textbackslash}xv\$, and their strains are spatially constant. Many materials, including all macroscopically isotropic amorphous ones, have elastic moduli that vary randomly with position, and they necessarily undergo nonaffine distortions in response to external stress. We study general aspects of nonaffine response and correlation using analytic calculations and numerical simulations. We define nonaffine displacements \${\textbackslash}uv' ({\textbackslash}xv)\$ as the difference between \${\textbackslash}uv ({\textbackslash}xv)\$ and affine displacements, and we investigate the nonaffinity correlation function \${\textbackslash}mathcal\{G\} = {\textless} [{\textbackslash}uv'({\textbackslash}xv) - {\textbackslash}uv' (0)]{\textasciicircum}2{\textgreater}\$ and related functions. We introduce four model random systems with random elastic moduli induced by locally random spring constants, by random coordination number, by random stress, or by any combination of these. We show analytically and numerically that \${\textbackslash}mathcal\{G\}\$ scales as \$A {\textbar}{\textbackslash}xv{\textbar}{\textasciicircum}\{-(d-2)\}\$ where the amplitude \$A\$ is proportional to the variance of local elastic moduli regardless of the origin of their randomness. We show that the driving force for nonaffine displacements is a spatial derivative of the random elastic constant tensor times the constant affine strain. Random stress by itself does not drive nonaffine response, though the randomness in elastic moduli it may generate does. We study models with both short and long-range correlations in random elastic moduli.},
	number = {6},
	urldate = {2014-08-18},
	journal = {Physical Review E},
	author = {DiDonna, B. A. and Lubensky, T. C.},
