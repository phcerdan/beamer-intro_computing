	keywords = {Analog computation, bioinformatics, Computer Appl. in Life Sciences, Embodiment, Morphological computation, neurobiology, Neurosciences, Nonlinear mass-spring systems, Statistical Physics, Dynamical Systems and Complexity, Volterra series},
