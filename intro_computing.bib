@article{walther_influence_2006,
	title = {Influence of κ-{Carrageenan} {Gel} {Structures} on the {Diffusion} of {Probe} {Molecules} {Determined} by {Transmission} {Electron} {Microscopy} and {NMR} {Diffusometry}},
