	keywords = {adjacency tree, binary digital image, binary digital picture, border, component, connectedness, continuous analog, digital fundamental group, digital topology, Euler characteristic, hole, Jordan curve, polyhedral analog, Strongly normal digital picture space, tunnel},
	pages = {219--262},
	file = {Kong_Concepts_of_digital_topology_1992_Topology_and_its_Applications.pdf:/home/phc/Dropbox/ResearchPapers/Topology and its Applications/1992/Kong_Concepts_of_digital_topology_1992_Topology_and_its_Applications.pdf:application/pdf}
